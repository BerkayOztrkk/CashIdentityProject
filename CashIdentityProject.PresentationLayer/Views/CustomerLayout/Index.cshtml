
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_CustomerLayoutHeaderPartialView")


<body class="nav-md footer_fixed">
    <div class="container body">
        <div class="main_container">
            
            @await Component.InvokeAsync("_CustomerLayoutSidebarPartialView")
            <!-- top navigation -->
            @await Component.InvokeAsync("_CustomerLayoutNavbarPartialView")
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                @RenderBody()
            </div>
            <!-- /page content -->
            <!-- footer content -->
            @await Component.InvokeAsync("_CustomerLayoutFooterPartialView")
            <!-- /footer content -->
        </div>
    </div>
    @await Component.InvokeAsync("_CustomerLayoutScriptPartialView")
</body>
</html>